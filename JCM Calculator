            "application/vnd.jupyter.widget-view+json": {
              "version_major": 2,
              "version_minor": 0,
              "model_id": "fae5cb8a60e444bfb170d1703ad343dd"
            }
          },
          "metadata": {}
        }
      ],
      "source": [
        "from IPython.display import display, HTML\n",
        "import ipywidgets as widgets\n",
        "\n",
        "# Data for aircraft categories and hourly rates based on deposit tiers\n",
        "data = {\n",
        "    \"Very Light\": {\"Retail\": 7400, \"$150k\": 6950, \"$300k\": 6850, \"$500k\": 6650},\n",
        "    \"Light\": {\"Retail\": 8000, \"$150k\": 7450, \"$300k\": 7300, \"$500k\": 7100},\n",
        "    \"Midsize\": {\"Retail\": 9200, \"$150k\": 8500, \"$300k\": 8350, \"$500k\": 8150},\n",
        "    \"Super-Midsize\": {\"Retail\": 10900, \"$150k\": 9850, \"$300k\": 9650, \"$500k\": 9400},\n",
        "    \"Super-Mid+\": {\"Retail\": 12750, \"$150k\": 11600, \"$300k\": 11350, \"$500k\": 11000},\n",
        "    \"Large\": {\"Retail\": 14200, \"$150k\": 13000, \"$300k\": 12800, \"$500k\": 12450},\n",
        "    \"Heavy\": {\"Retail\": 16150, \"$150k\": 14800, \"$300k\": 14500, \"$500k\": 13950},\n",
        "    \"Ultra\": {\"Retail\": 20200, \"$150k\": 18400, \"$300k\": 18100, \"$500k\": 17500}\n",
        "}\n",
        "\n",
        "# Function to calculate trip cost\n",
        "def calculate_cost(aircraft, deposit, flight_hours_list):\n",
        "    if aircraft not in data or deposit not in data[aircraft]:\n",
        "        return \"Invalid selection. Please choose valid options.\"\n",
        "\n",
        "    hourly_rate = data[aircraft][deposit]\n",
        "    total_cost = 0\n",
        "\n",
        "    for flight_hours in flight_hours_list:\n",
        "        if flight_hours > 5:\n",
        "            total_cost += hourly_rate * flight_hours * 0.80  # 20% off\n",
        "        elif flight_hours > 4:\n",
        "            total_cost += hourly_rate * flight_hours * 0.85  # 15% off\n",
        "        else:\n",
        "            total_cost += hourly_rate * flight_hours\n",
        "\n",
        "    return f\"The total cost for your trip is: <span style='color: green; font-weight: bold;'>${total_cost:,.2f}</span>\"\n",
        "\n",
        "# Widgets for user input\n",
        "aircraft_dropdown = widgets.Dropdown(\n",
        "    options=list(data.keys()),\n",
        "    value=\"Very Light\",\n",
        "    description=\"Aircraft Category:\",\n",
        "    style={\"description_width\": \"initial\"}\n",
        ")\n",
        "\n",
        "deposit_dropdown = widgets.Dropdown(\n",
        "    options=[\"Retail\", \"$150k\", \"$300k\", \"$500k\"],\n",
        "    value=\"Retail\",\n",
        "    description=\"Deposit Tier:\",\n",
        "    style={\"description_width\": \"initial\"}\n",
        ")\n",
        "\n",
        "leg_count_dropdown = widgets.Dropdown(\n",
        "    options=[str(i) for i in range(1, 11)],\n",
        "    value=\"1\",\n",
        "    description=\"Number of Legs:\",\n",
        "    style={\"description_width\": \"initial\"}\n",
        ")\n",
        "\n",
        "flight_hours_boxes_container = widgets.VBox()\n",
        "\n",
        "# Function to dynamically update flight hour text boxes\n",
        "def update_flight_hours(change):\n",
        "    global flight_hours_boxes_container\n",
        "    flight_hours_boxes = []\n",
        "    num_legs = int(change[\"new\"])\n",
        "    for i in range(num_legs):\n",
        "        box = widgets.FloatText(\n",
        "            value=1.0,\n",
        "            description=f\"Leg {i + 1} Hours:\",\n",
        "            style={\"description_width\": \"initial\"}\n",
        "        )\n",
        "        flight_hours_boxes.append(box)\n",
        "    flight_hours_boxes_container.children = flight_hours_boxes\n",
        "\n",
        "# Initialize the first leg correctly\n",
        "update_flight_hours({\"new\": \"1\"})\n",
        "leg_count_dropdown.observe(update_flight_hours, names=\"value\")\n",
        "\n",
        "output = widgets.Output()\n",
        "\n",
        "# Button to calculate cost\n",
        "def on_calculate_clicked(b):\n",
        "    with output:\n",
        "        output.clear_output()\n",
        "        aircraft = aircraft_dropdown.value\n",
        "        deposit = deposit_dropdown.value\n",
        "        flight_hours_list = [box.value for box in flight_hours_boxes_container.children]\n",
        "\n",
        "        result = calculate_cost(aircraft, deposit, flight_hours_list)\n",
        "        display(HTML(result))\n",
        "\n",
        "calculate_button = widgets.Button(\n",
        "    description=\"Calculate Cost\",\n",
        "    button_style=\"success\"\n",
        ")\n",
        "calculate_button.on_click(on_calculate_clicked)\n",
        "\n",
        "# Display the interface\n",
        "display(widgets.VBox([\n",
        "    aircraft_dropdown,\n",
        "    deposit_dropdown,\n",
        "    leg_count_dropdown,\n",
        "    flight_hours_boxes_container,\n",
        "    calculate_button,\n",
        "    output\n",
        "]))\n"
      ]
    }
  ]
}